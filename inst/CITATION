citHeader("To cite familial in publications use:")

bibentry(
  bibtype     = "Article",
  title       = "Familial inference: Tests for hypotheses on a family of centres",
  author      = c(as.person("Ryan Thompson"), as.person("Catherine S Forbes"), as.person("Steven N MacEachern"), as.person("Mario Peruggia")),
  journal     = "arXiv:2202.12540",
  year        = "2023"
)
